@article{Hidasi2016,
abstract = {We apply recurrent neural networks (RNN) on a new domain, namely recommender systems. Real-life recommender systems often face the problem of having to base recommendations only on short session-based data (e.g. a small sportsware website) instead of long user histories (as in the case of Netflix). In this situation the frequently praised matrix factorization approaches are not accurate. This problem is usually overcome in practice by resorting to item-to-item recommendations, i.e. recommending similar items. We argue that by modeling the whole session, more accurate recommendations can be provided. We therefore propose an RNN-based approach for session-based recommendations. Our approach also considers practical aspects of the task and introduces several modifications to classic RNNs such as a ranking loss function that make it more viable for this specific problem. Experimental results on two data-sets show marked improvements over widely used approaches.},
archivePrefix = {arXiv},
arxivId = {1511.06939},
author = {Hidasi, Bal{\'{a}}zs and Karatzoglou, Alexandros and Baltrunas, Linas and Tikk, Domonkos},
eprint = {1511.06939},
file = {:C$\backslash$:/Users/tph02/OneDrive/바탕 화면/참고논문/session based recommendations with reccurrent neural networks.pdf:pdf},
journal = {4th International Conference on Learning Representations, ICLR 2016 - Conference Track Proceedings},
pages = {1--10},
title = {{Session-based recommendations with recurrent neural networks}},
year = {2016}
}
@article{Dallmann2017,
abstract = {Recently, Recurrent Neural Networks (RNNs) have been applied to the task of session-based recommendation. These approaches use RNNs to predict the next item in a user session based on the previ- ously visited items. While some approaches consider additional item properties, we argue that item dwell time can be used as an implicit measure of user interest to improve session-based item recommen- dations. We propose an extension to existing RNN approaches that captures user dwell time in addition to the visited items and show that recommendation performance can be improved. Additionally, we investigate the usefulness of a single validation split for model selection in the case of minor improvements and find that in our case the best model is not selected and a fold-like study with different validation sets is necessary to ensure the selection of the best model.},
archivePrefix = {arXiv},
arxivId = {1706.10231},
author = {Dallmann, Alexander and Grimm, Alexander and P{\"{o}}litz, Christian and Zoller, Daniel and Hotho, Andreas},
eprint = {1706.10231},
file = {:C$\backslash$:/Users/tph02/OneDrive/바탕 화면/참고논문/Improving Session Recommendation with Recurrent Neural.pdf:pdf},
keywords = {(),dwell time,neural networks,recurrent neural networks,session recommendation,user interest modeling},
pages = {1--6},
title = {{Improving Session Recommendation with Recurrent Neural Networks by Exploiting Dwell Time}},
