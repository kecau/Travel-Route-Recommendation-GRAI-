file = {:C$\backslash$:/Users/tph02/OneDrive/바탕 화면/참고논문/A survey for trust-aware recommender systems.pdf:pdf},
issn = {09507051},
journal = {Knowledge-Based Systems},
keywords = {Deep learning,Recommender systems,Systematic survey},
